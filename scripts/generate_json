#!/usr/bin/perl

use warnings;
use strict;

use Digest::MD5 qw(md5 md5_hex md5_base64);

sub generate_unique_id {
    my $buildID = md5_base64 ( rand($$) ) . md5_base64 ( rand($$) ) . md5_base64 ( rand($$) );

    # Strip out "+", "/" and \d
    $buildID =~ s/\+//g;
    $buildID =~ s/\///g;

    return substr ($buildID, -12 );
}

